{
  "env": {
    "jest": true
  },
  "plugins": ["ava"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "space-before-function-paren": "off",
    "eol-last": "off",
    "no-trailing-spaces": "off",
    "curly": "off",
    "@typescript-eslint/no-inferrable-types": "off",
    "new-cap": "off",
    "@typescript-eslint/indent": "off",
    "indent": "off",
    "no-tabs": "off",
    "camelcase": "warn"
  }
}
